{
  "name": "tinypng-cli",
  "version": "1.0.0",
  "description": "Handy command line tool for shrinking PNG images using the TinyPNG API",
  "homepage": "https://github.com/karbassi/tinypng-cli",
  "authors": [
    "Ali Karbassi <ali@karbassi.com>",
    "websperts <hello@websperts.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:karbassi/tinypng-cli.git"
  },
  "keywords": [
    "tinypng",
    "shrinking",
    "compression",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/karbassi/tinypng-cli/issues"
  },
  "license": "MIT",
  "preferGlobal": true,
  "main": "./dist/index.js",
  "bin": {
    "tinypng": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/index.js",
    "lint": "eslint tinypng-cli.js",
    "format": "prettier --write tinypng-cli.js"
  },
  "dependencies": {
    "array-uniq": "^3.0.0",
    "chalk": "^5.2.0",
    "commander": "^10.0.1",
    "figlet": "^1.6.0",
    "glob": "^10.2.2",
    "md5-file": "^5.0.0",
    "minimatch": "^9.0.0",
    "minimist": "^1.2.8",
    "pretty-bytes": "^6.1.0",
    "request": "^2.70.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.5",
    "@types/node": "^18.16.1",
    "@types/request": "^2.48.8",
    "eslint": "^8.39.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
